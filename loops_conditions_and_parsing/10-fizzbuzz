# Script: FizzBuzz from 1 to 100
# Displays numbers in a list format
# Multiples of 3 → Fizz
# Multiples of 5 → Buzz
# Multiples of 3 and 5 → FizzBuzz

for i in {1..100}; do
  if (( i % 15 == 0 )); then
    echo "FizzBuzz"
  elif (( i % 3 == 0 )); then
    echo "Fizz"
  elif (( i % 5 == 0 )); then
    echo "Buzz"
  else
    echo "$i"
  fi
done
