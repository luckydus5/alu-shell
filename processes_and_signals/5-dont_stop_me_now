#!/usr/bin/env bash
# This script stops the 4-to_infinity_and_beyond process using its PID

# Find the PID(s) of the process
pid=$(pgrep -f 4-to_infinity_and_beyond)

# Kill the process if found
if [[ -n "$pid" ]]; then
  kill "$pid"
  echo "Process 4-to_infinity_and_beyond (PID $pid) has been stopped."
else
  echo "Process 4-to_infinity_and_beyond is not running."
fi
